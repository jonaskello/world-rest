{
  "name": "world-rest",
  "description": "REST API for world-db",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "run-s db-start server-start",
    "server-start": "tsc-watch --preserveWatchOutput --onSuccess \"node lib/server.js\" --onFailure \"echo >>> Compilation Failed\"",
    "client-start": "serve",
    "db-start": "docker start world-db",
    "db-run": "docker run --name world-db -d -p 5050:5432 ghusta/postgres-world-db:2.2-alpine",
    "db-stop": "docker stop world-db"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^12.7.5",
    "@types/pg": "^7.11.1",
    "npm-run-all": "^4.1.5",
    "serve": "^11.1.0",
    "source-map-support": "^0.5.13",
    "tsc-watch": "^3.0.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "pg": "^7.12.1"
  }
}